{
  "name": "fortrabbit/craft-auto-migrate",
  "description": "Runs migrations automatically",
  "keywords": ["composer", "development", "craftcms"],
  "type": "composer-plugin",
  "version": "1.2.0",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "fortrabbit\\CraftAutoMigrate\\": "src"
    }
  },
  "authors": [
    {
      "name": "Oliver Stark",
      "homepage": "https://www.fortrabbit.com/"
    }
  ],
  "require": {
    "composer-plugin-api": "^1.0",
    "craftcms/cms": "^3.0.0",
    "vlucas/phpdotenv": "^2.5 | ^3.4"
  },
  "require-dev": {
    "composer/composer": "^1.0"
  },
  "extra": {
    "class": "fortrabbit\\CraftAutoMigrate\\Plugin"
  }
}
